   #include<stdio.h>
     int main(){int n;
     scanf("%d",&n);

     int *p=malloc(n*sizeof(int));
     for(int i=0;i<n;i++)
     { scanf("%d",&p[i]);

     }

     int m=p[0],sm=p[0];
      for(int i=0;i<n;i++)
     {  if(p[i]>m){
         sm=m;
         m=p[i];

     }

     }
     printf("%d",sm);


     }

