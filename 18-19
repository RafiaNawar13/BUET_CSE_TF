








#include<stdio.h>//(18-19) 3c//string theke alphabetic and digit part alada kra

 #include<string.h>
  int main(){
  char s[51];
  char digitString[51];
   char alphaString[51];int j=0,k=0;
    scanf("%s",s);
  //  char s[51]=input;
    for(int i=0;s[i];i++)//for(int i=0;s[i]!=\0
    { if('0'<=s[i]&&s[i]<='9')
        digitString[j++]=s[i];
        else if(('a'<=s[i]&&s[i]<='z')||('A'<=s[i]&&s[i]<='Z'))
    alphaString[k++]=s[i];
    }
    printf("Digit String is%s\n",digitString);
     printf("Alpha String is\n%s\n",alphaString);




  }
   #include<stdio.h>
   int series(int n)//4b
   {  if(n==1) return 1;
    else if(n==2) return 2;
     else if(n==3) return 3;
      return series(n-1)+series(n-3);

   }


   int main(){


   int n;
   scanf("%d",&n);
    printf("%d\n",series(n));

   }
   #include<stdio.h>
     int main(){//2b
     int n;
     scanf("%d",&n);
     int p;
     scanf("%d",&p);int max=p,second_max=p;n--;
     while(n){
        scanf("%d",&p);
        if(p>max)  {second_max=max;
        max=p;
        }



     n--;}
     printf("%d\n",second_max);


     main();}

